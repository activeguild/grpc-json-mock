{
  "compilerOptions": {
    "baseUrl": "./",
    "moduleResolution": "node",
    "target": "es6",
    "module": "commonjs",
    "lib": ["dom", "esnext"],
    "experimentalDecorators": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictFunctionTypes": true
  }
}
