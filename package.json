{
  "name": "grpc-mocky",
  "version": "0.1.12",
  "description": "This is a mock of grpc used for javascript development.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activeguild/grpc-mocky.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
